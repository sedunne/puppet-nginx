{
  "name": "sedunne-nginx",
  "version": "0.1.0",
  "author": "sedunne",
  "summary": "Opinionated module to manage Ngnix and site files",
  "license": "Apache-2.0",
  "source": "https://github.com/sedunne/puppet-nginx",
  "project_page": "https://github.com/sedunne/puppet-nginx",
  "issues_url": "https://github.com/sedunne/puppet-nginx/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 2015.3.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 2.0.0 < 5.0.0"
    }
  ]
}
